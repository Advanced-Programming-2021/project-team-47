<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ProfileController">
   <children>
      <ImageView fx:id="profileImageView" fitHeight="128.0" fitWidth="114.0" layoutX="40.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="usernameLabel" layoutX="215.0" layoutY="28.0" text="Username">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="aliasTextField" layoutX="215.0" layoutY="67.0" promptText="please enter your alias" text="UserAlias" />
      <Button fx:id="passwordChangeButton" layoutX="451.0" layoutY="30.0" mnemonicParsing="false" onAction="#changePassword" text="Change Password" />
      <Button fx:id="profilePhotoChange" layoutX="40.0" layoutY="174.0" mnemonicParsing="false" onAction="#changePhoto" text="Change Profile Photo" />
   </children>
</AnchorPane>
