<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LoginController">
    <children>
        <TextField fx:id="username" layoutX="282.0" layoutY="133.0" onMouseClicked="#showUsername" />
        <PasswordField fx:id="password" layoutX="282.0" layoutY="244.0" onMouseClicked="#showPassword" />
        <Label layoutX="197.0" layoutY="195.0" text="Username: " />
        <Label layoutX="199.0" layoutY="249.0" text="Password: " />
        <Button layoutX="321.0" layoutY="343.0" mnemonicParsing="false" onMouseClicked="#loginResponse" prefHeight="41.0" prefWidth="108.0" text="Login" />
        <Button layoutX="321.0" layoutY="295.0" mnemonicParsing="false" onMouseClicked="#signUpResponse" prefHeight="41.0" prefWidth="108.0" text="SignUp" />
        <TextField fx:id="nickName" layoutX="282.0" layoutY="190.0" onMouseClicked="#showNickName" />
        <Label layoutX="51.0" layoutY="138.0" text="Nickname:(necessary for SignUp)" />
        <Button mnemonicParsing="false" onMouseClicked="#back" text="Back" />
    </children>
</AnchorPane>
